{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "babel nodejs starter",
  "author": "Tristan Teufel",
  "homepage": "https://teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/shenron/babel-node-starter.git"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "flow:deps": "flow-typed install jest@22.x",
    "dev": "nodemon src/index.js --exec babel-node --inspect-brk",
    "lint:fix": "eslint --fix src",
    "test": "jest tests/**",
    "serve": "npm run build && node dist/index.js",
    "build:webpack": "webpack --mode=production",
    "build:babel": "babel src -d dist",
    "build": "npm run build:webpack"
  },
  "dependencies" : {
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^0.1.19",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.47.1",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "flow-bin": "^0.73.0",
    "flow-typed": "^2.4.0",
    "jest": "^23.0.0",
    "nodemon": "^1.17.5"
  }
}
