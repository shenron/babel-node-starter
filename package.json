{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "babel nodejs starter",
  "homepage": "https://teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/shenron/babel-node-starter.git"
  },
  "license": "MIT",
  "author": "Tristan Teufel",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:babel",
    "build:babel": "./node_modules/.bin/babel src -d dist",
    "build:webpack": "webpack --mode=production",
    "dev": "babel-watch src/index.js",
    "flow": "flow",
    "flow:deps": "flow-typed install jest@22.x",
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "serve": "npm run build && node dist/index.js",
    "precommit": "lint-staged",
    "test": "jest tests/**"
  },
  "lint-staged": {
   "*.js": ["eslint --fix", "git add"]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-watch": "^7.0.0",
    "clean-webpack-plugin": "^1.0.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.93.0",
    "flow-typed": "^2.5.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.4",
    "nodemon": "^1.18.10",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
