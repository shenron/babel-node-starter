{
  "name": "babel-node-starter",
  "version": "1.0.0",
  "description": "babel nodejs starter",
  "homepage": "https://teufel-it.de",
  "repository": {
    "type": "git",
    "url": "https://github.com/shenron/babel-node-starter.git"
  },
  "license": "MIT",
  "author": "Tristan Teufel",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:babel",
    "build:babel": "./node_modules/.bin/babel src -d dist --source-maps",
    "build:webpack": "webpack --mode=production",
    "dev": "babel-watch src/index.js",
    "flow": "flow",
    "flow:deps": "flow-typed install jest@22.x",
    "lint": "eslint src tests",
    "lint:fix": "eslint --fix src tests",
    "serve": "npm run build && node dist/index.js",
    "precommit": "lint-staged",
    "test": "jest tests/**"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "dependencies": {
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-flow": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-watch": "^7.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-flowtype": "^4.5.2",
    "eslint-plugin-flowtype-errors": "^4.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "flow-bin": "^0.112.0",
    "flow-typed": "^2.6.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  }
}
